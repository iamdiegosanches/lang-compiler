minhaLista = new [5]; 

minhaLista[0] = 10;
minhaLista[1] = 20;
minhaLista[2] = minhaLista[0] + minhaLista[1]; -- Acessa elementos e realiza operação

print minhaLista[0]; -- Deve imprimir 10
print minhaLista[1]; -- Deve imprimir 20
print minhaLista[2]; -- Deve imprimir 30 (10 + 20)

outraLista = new [3];
outraLista[0] = true; -- A primeira atribuição (Bool) definirá outraLista como Bool[]
outraLista[1] = false;
outraLista[2] = false;
print outraLista[0];  -- Deve imprimir true
print outraLista[1];  -- Deve imprimir false

print 100; -- Separador para saída
iterate (i : 3) {
    print i; -- Deve imprimir 3, 2, 1
}

iterate (valor : outraLista) {
    print valor;
}

