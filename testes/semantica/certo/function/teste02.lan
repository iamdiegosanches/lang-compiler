readArr( v :: Int[], sz :: Int) : Int[]{
    i = 0;
    x = 0;
    iterate(sz){
        read x;
        v[i] = x;
        i = i + 1;
    }
    return v;
}

printArr(v :: Int [],  sz :: Int){
    print '{';
    if(0 < sz){
        print v[0];
        i = 1;
        iterate(sz-1){
            print ',';
            print v[i];
            i = i + 1;
        }
    }
    print '}';
    print '\n';
}

main() {
    x = 0;
    v = new Int[10];
    readArr(v,10)<v>;
    printArr(v,10);
}